{
  "name": "faithful-force",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro check && astro build",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "astro preview",
    "astro": "astro",
    "prepare": "husky"
  },
  "dependencies": {
    "astro": "5.6.0"
  },
  "devDependencies": {
    "@astrojs/check": "0.9.4",
    "@typescript-eslint/parser": "8.29.0",
    "eslint": "9.23.0",
    "eslint-plugin-astro": "1.3.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "husky": "9.1.7",
    "lint-staged": "15.5.0",
    "prettier": "3.5.3",
    "prettier-plugin-astro": "0.14.1",
    "typescript": "5.8.2"
  },
  "lint-staged": {
    "*.{astro,js,ts,tsx,json}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
